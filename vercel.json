{
  "routes": [
    {
      "src": "/ingest/(.*)",
      "dest": "https://us.i.posthog.com/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/ingest/static/(.*)",
      "dest": "https://us-assets.i.posthog.com/static/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
